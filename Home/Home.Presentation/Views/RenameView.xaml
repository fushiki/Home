<Window x:Class="Home.Presentation.Views.RenameView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Home.Presentation.Views"
        mc:Ignorable="d"
        Title="RenameView" Height="150" Width="200">
    <DockPanel>
        <StackPanel DockPanel.Dock="Top">
            <Label Content="Name"/>
            <TextBox Height="23" TextWrapping="Wrap" Text="{Binding Name}" Width="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>

        </StackPanel>
        <Button DockPanel.Dock="Bottom" Command="{Binding CommandOK}" Content="OK" VerticalAlignment="Bottom" Width="75" HorizontalAlignment="Right"/>
    </DockPanel>
</Window>
